class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
class Stack:
    def __init__(self):
        self.head=None
    def empty(self):
        return True if self.head == None else False
    def push (self,data):
        if self.head==None:
            self.head=Node(data)
        else:
            new_node=Node(data)
            new_node.next=self.head
            new_node=self.head
    def TE(self):
        if self.empty():
            return None
        else:
            return self.head.data
    def pop(self):
        if self.empty():
            return None
        else:
            popped_node=self.head
            self.head=self.head.next
            popped_node.next=None
            return popped_node.data
a=Stack()
a.push(5)
a.push(2)
a.push(7)
a.push(8)
print("top element :")
print(a.TE())
print("Popped element:")
print(a.pop())

    
    
