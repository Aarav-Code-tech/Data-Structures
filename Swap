


class node:
    def __init__(self,data):
        self.data=data
        self.next=None
def swap(head):
        if head is None or head.next is None:
            return head
        first=head 
        second =head.next
        first.next=second.next
        second.next=first
        return second
def print_list(head):
        temp=head
        while temp:
            print(temp.data,end="-->")
            temp=temp.next
        print("None")
head=node(1)
head.next=node(2)
head.next.next=node(3)
print("before:")
print_list(head)
print("after:")
head=swap(head)
print_list(head)




    
